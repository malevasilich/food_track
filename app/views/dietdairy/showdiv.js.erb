$("#dietdairy_div").html("<%=j render 'dietdairydb' %>");

$('#dietdairy_div tr').click(function(){
 $('#new_food_name').val(this.cells[0].textContent);
 $('#new_food_p').val(this.cells[1].textContent);
 $('#new_food_f').val(this.cells[2].textContent);
 $('#new_food_c').val(this.cells[3].textContent);
 $('#new_food_w').val(this.cells[4].textContent);
 $('#new_food_kk').val(this.cells[5].textContent);
});

$('#dietdairy_div tr').dblclick(function(){
	// assume already has been clicked once and the fields values are in place 
	$('#new_food_form').submit();
});

